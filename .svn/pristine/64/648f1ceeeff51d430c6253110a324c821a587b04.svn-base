	-webkit-box-shadow: 2px 2px 3px #688E6D;
	box-shadow: 2px 2px 3px #688E6D;
	/*filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#EFF5FF,endColorstr=#E0ECFF,GradientType=0);
	
*/}

.panelXqTitle a{
	width:16px;
	height:16px;
	-moz-user-select:none;
	-moz-user-focus:none;
	float:right;
	background:url(../images/panel_tools.png);
	background-position: -16px 0;
	top:5px;
	opacity: 0.6;
	filter: alpha(opacity=60);
	display: inline-block;
	margin: 0 10;
	cursor: pointer;
	position: absolute;
	right: 5px;
}

.panelXqTitle a:hover {
	opacity: 1;
	filter: alpha(opacity=100);
	display: inline-block;
}

.cds_btn {
  text-decoration: none;
  display: inline-block;
  overflow: hidden;
  margin: 0;
  padding: 0px 5px;
  cursor: pointer;
  outline: none;
  text-align: center;
  vertical-align: middle;
  width:auto;
  color: #444;
  background: #fafafa;
  background-repeat: repeat-x;
  border: 1px solid #bbb;
  background: -webkit-linear-gradient(top,#ffffff 0,#eeeeee 100%);
  background: -moz-linear-gradient(top,#ffffff 0,#eeeeee 100%);
  background: -o-linear-gradient(top,#ffffff 0,#eeeeee 100%);
  background: linear-gradient(to bottom,#ffffff 0,#eeeeee 100%);
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#ffffff,endColorstr=#eeeeee,GradientType=0);
  /*-moz-border-radius: 5px 5px 5px 5px;
  -webkit-border-radius: 5px 5px 5px 5px;
  border-radius: 5px 5px 5px 5px;
*/}


.cds_btn:hover{
	background: #EAF2FF;
	color: #000;
	border: 1px solid #B7D2FF;
	filter: none;
}

.cds_btn_text {
    color: rgb(68, 68, 68);
    cursor: pointer;
    display: inline-block;
    height: 24px;
    text-align: center;
    vertical-align: top;
    width:100%;
    perspective-origin: 15.828125px 12px;
    transform-origin: 15.828125px 12px;
    border: 0px none rgb(68, 68, 68);
    font: normal normal normal 12px/24px verdana, helvetica, arial, sans-serif;
    outline: rgb(68, 68, 68) none 0px;
}


