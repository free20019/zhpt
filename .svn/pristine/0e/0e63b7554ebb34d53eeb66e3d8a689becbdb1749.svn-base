		.clearfix:after { display: block; clear: both; height: 0; content: ''; visibility: hidden; }

		.panel {
			margin-bottom: 20px;
			background-color: #fff;
			border: 1px solid #ddd;
			border-radius: 4px;
			-webkit-box-shadow: 0 1px 1px rgba(0,0,0,.05);
			box-shadow: 0 1px 1px rgba(0,0,0,.05);
		}
		.panel-heading {
			padding: 10px 15px;
			border-bottom: 1px solid #ddd;
			border-top-left-radius: 3px;
			border-top-right-radius: 3px;
			font-size: 18px;
			color: #333;
		}
		.panel-body > .panel-heading,
		.panel-body > .item > .panel-heading,
		.panel-body > [class^='item-'] > .panel-heading {
			margin-top: -15px;
		}
		.panel-body {
			padding: 15px;
		}
		.item {
			float: left;
			margin-right: 15px;
		}
		.item.item-c2 {
			width: calc(50% - 7.5px);
		}
		.item.item-c2:nth-child(2n) {
			margin-right: 0;
		}
		.item.item-c2:nth-last-child(1),
		.item.item-c2:nth-last-child(2) {
			margin-bottom: 0;
		}
		.item.item-c3 {
			width: calc(33.33% - 12px);
		}
		.item.item-c3:nth-child(3n) {
			margin-right: 0;
		}
		.item.item-c3:nth-last-child(1),
		.item.item-c3:nth-last-child(2),
		.item.item-c3:nth-last-child(3) {
			margin-bottom: 0;
		}
		.panel-heading .item {
			height: 20px;
			line-height: 20px;
			border-right: 1px solid #dddddd;
			text-align: center;
			box-sizing: border-box;
		}
		.panel-heading .item:last-child {
			border-right: none;
		}
		.highlight,
		[class^='highlight-'] {
			font-size: 140%;
		}
		.highlight-red {
			color: #fb5d8e;
		}
		.highlight-orange {
			color: #fc9c3c;
		}
		.highlight-green {
