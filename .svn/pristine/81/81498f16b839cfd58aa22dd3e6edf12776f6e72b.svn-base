				<ul id="yjyaList" > </ul>
			</div>
		</div>

		<div id="yjzhYjyaContent" data-dojo-type="dijit/layout/ContentPane"
			data-dojo-props=" region:'center'"
			style="height:90%;padding: 0px;margin: 0px;">
